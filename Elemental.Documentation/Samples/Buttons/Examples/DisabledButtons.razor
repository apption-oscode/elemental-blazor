@using Elemental.Components.Button

<AeButton To="/" Disabled>Default</AeButton>
<AeButton Disabled class="dark">Dark</AeButton>
<AeButton Disabled class="light">Light</AeButton>
<AeButton Disabled class="blue">Blue</AeButton>
<AeButton Disabled class="red">Red</AeButton>